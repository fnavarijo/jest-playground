{
  "name": "jest-playground",
  "version": "0.0.1",
  "description": "Jest playground",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "commit": "cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fnavarijo/jest-playground.git"
  },
  "keywords": [
    "jest"
  ],
  "author": "fernandonavarijo1996@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fnavarijo/jest-playground/issues"
  },
  "homepage": "https://github.com/fnavarijo/jest-playground#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "cz-emoji": "^1.3.1",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    },
    "cz-emoji": {
      "scopes": [
        "config",
        "matchers",
        "setup",
        "mocks",
        "snapshot",
        "coverage",
        "integration",
        "base"
      ]
    }
  }
